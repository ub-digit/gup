{{#each itemStates as |itemState index|}}
  {{!#collapsible-item tagName=itemTagName item=item activeItems=activeItems as |isActive|}}
  {{yield
    itemState.item
    (hash
      toggleIsActive=(action "toggleActiveItem" itemState.item)
      isActive=itemState.isActive
      index=index
    )
  }}
  {{!/collapsible-item}}
{{/each}}
