        {{/if}}

        {{#if allFieldObjects.patent_application_date}}
          {{publication-field-compare fieldLabel=allFieldObjects.patent_application_date.label otherIsSelected=otherIsSelected otherFieldValue=otherPublication.patent_application_date fieldValue=publication.patent_application_date}}
        {{/if}}

        {{#if allFieldObjects.patent_number}}
          {{publication-field-compare fieldLabel=allFieldObjects.patent_number.label otherIsSelected=otherIsSelected otherFieldValue=otherPublication.patent_number fieldValue=publication.patent_number}}
        {{/if}}

        {{#if allFieldObjects.patent_date}}
          {{publication-field-compare fieldLabel=allFieldObjects.patent_date.label otherIsSelected=otherIsSelected otherFieldValue=otherPublication.patent_date fieldValue=publication.patent_date}}
        {{/if}}

        {{#if allFieldObjects.artwork_type}}
          {{publication-field-compare fieldLabel=allFieldObjects.artwork_type.label otherIsSelected=otherIsSelected otherFieldValue=otherPublication.artwork_type fieldValue=publication.artwork_type}}
        {{/if}}
      </tbody>
    </table>
  {{else if isPreviewMode}}
      <div class="publication-display-top-meta lead">
      {{publication.publication_type_label}}{{#if isRef}}, {{getLowercaseRefValue}}{{/if}}
      </div>
      {{#if publication.epub_ahead_of_print}}
        <div class="alert alert-warning">
        {{t 'publications.dashboard.manage.show.index.epubAheadOfPrintSince'}}
        </div>
      {{/if}}

    <dl class="dl-horizontal">
      {{#if isArticleConf}}
        {{partial "publications/dashboard/manage/show/is-article-conf"}}
      {{/if}}

      {{#if isThesis}}
        {{partial "publications/dashboard/manage/show/is-thesis"}}
      {{/if}}

      {{#if isBook}}
        {{partial "publications/dashboard/manage/show/is-book"}}
      {{/if}}

      {{#if isChapter}}
        {{partial "publications/dashboard/manage/show/is-chapter"}}
      {{/if}}


      {{#if isArt}}
        {{partial "publications/dashboard/manage/show/is-art"}}
      {{/if}}

      {{#if isPatent}}
